/* components/bottom-model/bottom-model.wxss */
@import "../../iconfont.wxss";
.modal{
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 99;
  text-align: center;
  background: rgba(0, 0, 0, 0.6);
}

.panel{
  position: absolute;
  left: 0;
  width: 100%;
  bottom: 0;
  min-height: 300rpx;
  box-sizing: border-box;
  background-color: #f8f8f8;
}

.icon-shanchu{
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  padding: 20rpx;
}